{
  "license": "MIT",
  "scripts": {
    "postinstall": "install-app-deps",
    "dev:webpack": "webpack-dev-server --config ./webpack.dev.config.js",
    "dev:electron": "DEBUG=sqlectron-gui:*,sqlectron-core:*,spawn-auto-restart node scripts/dev-auto-restart.js",
    "test": "eslint .",
    "clean": "rimraf dist",
    "clean:osx": "rimraf dist/Sqlectron-darwin-*",
    "clean:win": "rimraf dist/win* dist/Sqlectron-win*",
    "clean:linux": "rimraf dist/Sqlectron-linux-* dist/*.deb",
    "compile": "rimraf app/out && NODE_ENV=production babel-node scripts/compile.js -v",
    "build:osx": "npm run clean:osx && build --platform darwin",
    "build:linux": "npm run clean:linux && build --platform linux --arch all",
    "build:win": "npm run clean:win && build --platform win32 --arch all",
    "dist:osx": "npm run compile && npm run build:osx",
    "dist:winlinux": "npm run compile && npm run build:linux && npm run build:win"
  },
  "build": {
    "app-bundle-id": "org.sqlectron.gui",
    "app-category-type": "public.app-category.developer-tools",
    "osx": {
      "contents": [
        {
          "x": 438,
          "y": 275,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 192,
          "y": 275,
          "type": "file"
        }
      ]
    }
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.29",
    "babel-core": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.29",
    "brace": "^0.7.0",
    "breakpoint-sass": "^2.6.1",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "csv-stringify": "0.0.8",
    "del": "^2.2.0",
    "denodeify": "^1.2.1",
    "electron-builder": "^3.5.2",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.37.5",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.15.0",
    "file-loader": "^0.8.5",
    "history": "^1.12.6",
    "html-webpack-plugin": "^1.6.2",
    "imports-loader": "^0.6.5",
    "jquery": "^2.2.0",
    "lodash": "^3.10.1",
    "minimist": "^1.2.0",
    "ncp": "^2.0.0",
    "node-sass": "^3.4.2",
    "object-assign": "^4.0.1",
    "react": "^0.14.6",
    "react-ace": "^2.8.0",
    "react-dom": "^0.14.3",
    "react-paginate": "^0.5.0",
    "react-redux": "^4.0.1",
    "react-resizable": "^1.3.2",
    "react-router": "^1.0.2",
    "react-select": "^0.9.1",
    "react-tabs": "^0.5.1",
    "react-transform-hmr": "^1.0.1",
    "redux": "^3.0.5",
    "redux-logger": "^2.2.1",
    "redux-thunk": "^1.0.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "spawn-auto-restart": "^2.0.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "git+https://github.com/maxcnunes/webpack.git#v1.12.8-electron",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.14.1"
  }
}
